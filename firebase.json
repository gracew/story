{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/signup",
        "destination": "https://voicebar.typeform.com/to/NUynD2GT",
        "type": 301
      },
      {
        "source": "/feedback",
        "destination": "https://voicebar.typeform.com/to/aryQh6Da",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/app/index.html"
      }
    ]
  }
}
